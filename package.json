{
  "name": "theme-dracula-clean",
  "displayName": "Dracula Clean",
  "description": "Dracula theme with better contrast or none at all!",
  "version": "4.0.5",
  "preview": true,
  "publisher": "echevarriandre",
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/echevarriandre/dracula-clean.git"
  },
  "bugs": {
    "url": "https://github.com/echevarriandre/dracula-clean/issues"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "dracula",
    "dark",
    "theme",
    "color-theme",
    "clean",
    "contrast",
    "better"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Dracula Clean",
        "uiTheme": "vs-dark",
        "path": "./themes/dracula-clean.json"
      },
      {
        "label": "Dracula Cleaner",
        "uiTheme": "vs-dark",
        "path": "./themes/dracula-cleaner.json"
      }
    ]
  },
  "galleryBanner": {
    "color": "#3c4557",
    "theme": "dark"
  },
  "devDependencies": {
    "chroma-js": "^2.1.0",
    "color": "^3.1.2",
    "nodemon": "^2.0.3",
    "vsce": "^2.2.0"
  },
  "scripts": {
    "start": "nodemon --watch src src/index.js",
    "build": "node src/index.js",
    "package": "vsce package -o ./themes/dracula-clean.vsix"
  }
}
